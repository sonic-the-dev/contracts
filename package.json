{
  "name": "hardhat-project",
  "scripts": {
    "compile": "hardhat compile --force",
    "typechain": "hardhat typechain",
    "test": "hardhat test",
    "build": "yarn compile && yarn typechain && yarn test",
    "deploy:dev:game": "hardhat run scripts/deploy-game.ts --network arbitrumGoerli",
    "deploy:dev:bridge": "hardhat run scripts/deploy-bridge.ts --network sepolia",
    "deploy:dev": "yarn deploy:dev:game && yarn deploy:dev:bridge",
    "deploy:prod:game": "hardhat run scripts/deploy-game.ts --network arbitrum",
    "deploy:prod:bridge": "hardhat run scripts/deploy-bridge.ts --network ethereum",
    "deploy:prod": "yarn deploy:prod:game && yarn deploy:prod:bridge",
    "lint": "eslint --ext .js,.ts,.tsx ."
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "@chainlink/contracts": "^0.6.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.1",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.4",
    "@nomiclabs/hardhat-vyper": "^3.0.3",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "chai": "^4.3.7",
    "eslint": "^8.45.0",
    "eslint-config-algolia": "^22.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-prettier": "^5.0.0",
    "ethers": "^6.6.5",
    "hardhat": "^2.17.0",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.0",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
